import java.awt.*;
import javax.swing.*;
  class DemoGUI
  {
    public static void main(String args[])
    {
      JFrame frm=new JFrame();
      Container con=new frm.getContentPane();
      FlowLayout flow=new FlowLayout();
      con.setLayout(flow);
      JButton btn=new JButton("Click Me");
      con.add(btn);
      frm.setSize(340,400);
      frm.setVisible(true);
     }
   }
      
